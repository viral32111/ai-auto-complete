{
	"version": "0.2.0",
	"configurations": [
		{
			"type": "node",
			"request": "launch",
			"name": "Launch Server",
			"skipFiles": [
				"<node_internals>/**"
			],
			"program": "${workspaceFolder}\\dist\\index.js",
			"preLaunchTask": "Build",
			"outFiles": [
				"${workspaceFolder}/dist/**/*.js"
			],
			"internalConsoleOptions": "openOnSessionStart"
		}
	]
}
